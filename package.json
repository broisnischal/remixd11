{
  "private": true,
  "name": "nees",
  "type": "module",
  "description": "All-in-one remix starter template for Cloudflare Pages",
  "scripts": {
    "dev": "remix vite:dev",
    "devh": "remix vite:dev --host",
    "test": "playwright test --ui",
    "start": "wrangler pages dev ./build/client",
    "build": "remix vite:build",
    "deploy": "npm run build && wrangler pages deploy",
    "deployminify": "npm run build && wrangler pages deploy --experimental-minify",
    "cleanup": "rimraf .cache ./build",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "wrangler types && tsc",
    "prepare": "husky",
    "typegen": "wrangler types",
    "preview": "npm run build && wrangler pages dev",
    "cf-typegen": "wrangler types",
    "generate": "npx drizzle-kit generate",
    "up": "drizzle-kit up:sqlite --schema=./app/drizzle/schema.server.ts",
    "migrate": "npx wrangler d1 migrations create dbs new",
    "migrate:apply": "npx wrangler d1 migrations apply dbs --remote",
    "migrate:local": "npx wrangler d1 migrations apply dbs --local",
    "studio": "npx drizzle-kit studio",
    "build:icons": "tsx ./other/build-icons.ts",
    "drop": "npx drizzle-kit drop"
  },
  "dependencies": {
    "@conform-to/react": "^1.1.5",
    "@conform-to/zod": "^1.1.5",
    "@epic-web/cachified": "^5.2.0",
