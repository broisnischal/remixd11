    "match-sorter": "^6.3.4",
    "moment": "^2.30.1",
    "next-themes": "^0.3.0",
    "ni": "^0.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "react-element-to-jsx-string": "^15.0.0",
    "react-i18next": "^14.1.2",
    "react-icon-cloud": "^4.1.4",
