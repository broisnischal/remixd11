{
  "private": true,
  "name": "nees",
  "type": "module",
  "description": "All-in-one remix starter template for Cloudflare Pages",
  "scripts": {
    "dev": "remix vite:dev",
    "test": "playwright test --ui",
    "start": "wrangler pages dev ./build/client",
    "build": "remix vite:build",
    "deploy": "npm run build && wrangler pages deploy",
    "cleanup": "rimraf .cache ./build",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "wrangler types && tsc",
    "prepare": "husky",
    "typegen": "wrangler types",
    "preview": "npm run build && wrangler pages dev",
    "cf-typegen": "wrangler types",
    "generate": "npx drizzle-kit generate",
    "up": "drizzle-kit up:sqlite --schema=./app/drizzle/schema.server.ts",
    "migrate": "npx wrangler d1 migrations create dbs new",
    "migrate:apply": "npx wrangler d1 migrations apply dbs --remote"
  },
  "dependencies": {
    "@markdoc/markdoc": "^0.4.0",
    "@mdx-js/rollup": "^3.0.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@remix-run/cloudflare": "^2.8.1",
    "@remix-run/cloudflare-pages": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@upstash/kafka": "^1.3.5",
    "@upstash/ratelimit": "^1.2.1",
    "@upstash/redis": "^1.32.0",
    "@use-gesture/react": "^10.3.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.31.2",
    "framer-motion": "^11.2.13",
    "isbot": "^3.6.5",
    "lucide-react": "^0.406.0",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "react-use-measure": "^2.1.1",
    "rehype-highlight": "^7.0.0",
    "rehype-pretty-code": "^0.12.3",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "vite-plugin-markdown": "^2.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240222.0",
    "@octokit/types": "^12.6.0",
    "@playwright/test": "^1.42.1",
    "@remix-run/dev": "^2.8.1",
    "@remix-run/eslint-config": "^2.8.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.22.8",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.3",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^5.0.0",
    "rimraf": "^5.0.5",
    "shikiji": "^0.10.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.2",
    "vite": "^5.1.5",
    "vite-tsconfig-paths": "^4.3.1",
    "wrangler": "^3.32.0"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "lint-staged": {
    "*.{js,mjs,ts,tsx,css,md,yml}": "prettier --write"
  }
}
